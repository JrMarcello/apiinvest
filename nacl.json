[
  {
    "group": "Admin",
    "permissions": [
      {
            "resource": "*",
            "methods": "*",
            "action": "allow"
      }
    ]
  },
  {
    "group": "Investidor",
    "permissions": [
      {
        "resource": "user/me",
        "methods": ["GET", "PUT", "DELETE"],
        "action": "allow"
      },
      {
        "resource": "investment/me",
        "methods": ["GET"],
        "action": "allow"
      },
      {
        "resource": "investment/",
        "methods": ["POST"],
        "action": "allow"
      },
      {
        "resource": "investment/ted",
        "methods": ["PUT"],
        "action": "allow"
      },
      {
        "resource": "investment/:id/cancel",
        "methods": ["PUT"],
        "action": "allow"
      },
      {
        "resource": "building/:id",
        "methods": ["GET"],
        "action": "allow"
      },
      {
        "resource": "building/available/",
        "methods": ["GET"],
        "action": "allow"
      }
    ]
  }
]
  